<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(function(message) { %>
      <li>
        <p>User: <%= message.user %></p>
        <p>Message: <%= message.text %></p>
        <p>Date: <%= message.added %></p>
      </li>
      <% }); %>
    </ul>
    <a href="http://localhost:3000/new">Add a New Message</a>
  </body>
</html>





